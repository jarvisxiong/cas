LoadPlugin write_graphite
<Plugin write_graphite>
  <Node "graphite_prod">
    Host "<%= graphite_host %>"
    Port "<%= graphite_port %>"
    Protocol "tcp"
    LogSendErrors true
    Prefix "<%= sys_graphite_prefix %>."
    StoreRates true
    AlwaysAppendDS false
    EscapeCharacter "_"
  </Node>
</Plugin>
