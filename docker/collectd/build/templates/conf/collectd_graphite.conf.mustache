Hostname "{{CONCISE_CONTAINER_NAME}}"
LoadPlugin write_graphite
<Plugin write_graphite>
    <Node "graphite_prod">
    Host "cas-metrics-relay.{{COLO}}.inmobi.com"
    Port "2020"
    Protocol "tcp"
    LogSendErrors true
    Prefix "{{graphite_prefix_base}}.{{COLO}}.{{IDP_SERVICE}}-1.sys."
    StoreRates true
    AlwaysAppendDS false
    EscapeCharacter "_"
    </Node>
</Plugin>

<Plugin tcpconns>
    ListeningPorts false
    LocalPort "8800"
    LocalPort "8801"
    RemotePort "3000"
    RemotePort "12555"
</Plugin>

